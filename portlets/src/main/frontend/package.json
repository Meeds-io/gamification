{
  "name": "Gamification",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "//": "Option --progress: print compilation progress in percentage",
    "start": "webpack-dashboard webpack-dev-server -d --hot --config webpack.dev.js --watch",
    "dev": "webpack --mode development --config webpack.dev.js",
    "watch": "webpack --config webpack.dev.js --progress --colors --watch -d",
    "build": "webpack -p --progress --trace-deprecation --config webpack.prod.js",
    "lint": "eslint --fix ./src/apps/*.js  ./src/apps/**/*.js ./src/apps/components/**/*.vue ./src/apps/components/**/**/*.vue",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check"
  },
  "author": "eXo",
  "license": "LGPL",
  "devDependencies": {
    "@babel/runtime": "^7.3.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "eslint": "^5.10.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.0.0",
    "jest-runner-eslint": "^0.5.0",
    "html-webpack-plugin": "^3.2.0",
    "vue-loader": "^14.2.2",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.13.0",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.3"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap-vue": "^2.0.0-rc.8",
    "jquery": "^3.2.1",
    "vue": "^2.5.16",
    "vue-avatar": "^2.1.4",
    "vue-bootstrap-datetimepicker": "^4.1.3",
    "vue-d2b": "^1.0.11",
    "vue-i18n": "^7.8.1",
    "vue-image-compare": "^0.6.1",
    "vue-moment": "^3.2.0",
    "vue-popperjs": "^2.2.0",
    "vue-table-component": "^1.9.2",
