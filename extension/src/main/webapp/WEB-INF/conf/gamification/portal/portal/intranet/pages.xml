<page-set
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.gatein.org/xml/ns/gatein_objects_1_3 http://www.gatein.org/xml/ns/gatein_objects_1_3"
        xmlns="http://www.gatein.org/xml/ns/gatein_objects_1_3">

  <page>
    <name>gameficationhelp</name>
    <title>How to earn gamification points</title>
    <access-permissions>Everyone</access-permissions>
    <edit-permission>*:/platform/administrators</edit-permission>
    <portlet-application>
      <portlet>
        <application-ref>presentation</application-ref>
        <portlet-ref>SingleContentViewer</portlet-ref>
        <preferences>
          <preference>
            <name>workspace</name>
            <value>collaboration</value>
            <read-only>false</read-only>
          </preference>
          <preference>
            <name>nodeIdentifier</name>
            <value>/sites/intranet/web contents/site artifacts/How can I earn points ?</value>
            <read-only>false</read-only>
          </preference>
          <preference>
            <name>ShowPrintAction</name>
            <value>false</value>
            <read-only>false</read-only>
          </preference>
          <preference>
            <name>ShowTitle</name>
            <value>false</value>
            <read-only>false</read-only>
          </preference>
          <preference>
            <name>ShowDate</name>
            <value>false</value>
            <read-only>false</read-only>
          </preference>
          <preference>
            <name>ShowOptionBar</name>
            <value>false</value>
            <read-only>false</read-only>
          </preference>
          <preference>
            <name>ContextEnable</name>
            <value>true</value>
            <read-only>false</read-only>
          </preference>
          <preference>
            <name>ParameterName</name>
            <value>content-id</value>
            <read-only>false</read-only>
          </preference>
        </preferences>
      </portlet>
      <title>Detail</title>
      <access-permissions>Everyone</access-permissions>
      <show-info-bar>false</show-info-bar>
      <show-application-state>false</show-application-state>
      <show-application-mode>false</show-application-mode>
    </portlet-application>
  </page>
  </page-set>
